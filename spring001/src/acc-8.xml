<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">
	
	<bean id="helloWorld" class="org.denny.hello.HelloWorld">
		<constructor-arg value="24" type="int" />
		<constructor-arg value="24" type="double" />
		<constructor-arg value="24" type="float" />
		<constructor-arg value="24" type="long" />
		<constructor-arg value="world !!!" type="String" index="4" />
	</bean>
 		
</beans>